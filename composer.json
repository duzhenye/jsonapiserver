{
    "name": "duzhenye/jsonapiserver",
    "description": "一个用 PHP 实现的完全自动化的 JSON:API 服务器。",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "asispts/http-accept": "^1.0",
        "doctrine/inflector": "^2.0",
        "nyholm/psr7": "^1.8",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Du"
        }
    ],
    "autoload": {
        "psr-4": {
            "Duzhenye\\JsonApiServer\\": "src/"
        },
        "files": [
            "src/functions.php",
            "src/Laravel/functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
